    <xsd:complexType name="com.ibm.ws.security.jca.internal.authdata.config"> 
        <xsd:annotation> 
            <xsd:documentation>Authentifizierungsdaten für eine Verbindung zu einem unternehmensweiten Informationssystem (EIS, Enterprise Information System).</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Authentifizierungsdaten</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="user" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Der Name des Benutzers, der für die Herstellung der Verbindung zum unternehmensweiten Informationssystem verwendet werden soll.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Benutzername</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
