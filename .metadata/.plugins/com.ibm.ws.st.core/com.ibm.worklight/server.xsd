            <xsd:element name="contextPool" type="com.ibm.ws.security.wim.adapter.ldap.contextPool"> 
                <xsd:annotation> 
                    <xsd:documentation>Die Eigenschaften des Kontextpools.</xsd:documentation> 
                    <xsd:appinfo> 
