            <xsd:element name="ldapCache" type="com.ibm.ws.security.wim.adapter.ldap.cacheConfig"> 
                <xsd:annotation> 
                    <xsd:documentation>Konfigurieren Sie die Attribute des Caches.</xsd:documentation> 
                    <xsd:appinfo> 
