        <xsd:attribute name="userFilter" use="optional" default="(&amp;(uid=%v)(objectclass=ePerson))" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Eine LDAP-Filterklausel für das Suchen von Benutzern in der Benutzerregistry.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Benutzerfilter</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupFilter" use="optional" default="(&amp;(cn=%v)(|(objectclass=groupOfNames)(objectclass=groupOfUniqueNames)))" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Eine LDAP-Filterklausel für das Suchen von Gruppen in der Benutzerregistry.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Gruppenfilter</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userIdMap" use="optional" default="*:uid" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Ein LDAP-Filter, der den Namen eines Benutzers einem LDAP-Eintrag zuordnet.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Zuordnung von Benutzer-IDs</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupIdMap" use="optional" default="*:cn" type="xsd:string"> 
