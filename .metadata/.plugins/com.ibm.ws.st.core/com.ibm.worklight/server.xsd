            <xsd:appinfo> 
                <ext:label>JAAS-Anmeldekontexteintrag</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
