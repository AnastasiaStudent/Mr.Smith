<?xml version="1.0" encoding="UTF-8"?> 
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ext="http://www.ibm.com/xmlns/dde/schema/annotation/ext"> 
    <xsd:complexType name="com.ibm.ws.security.authentication.tai.interceptor"> 
        <xsd:annotation> 
            <xsd:documentation>Definiert einen Trust-Association-Interceptor (TAI).</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Trust-AssociationInterceptor</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="library" type="com.ibm.ws.classloading.sharedlibrary"> 
                <xsd:annotation> 
                    <xsd:documentation>Eine Referenz auf die ID der gemeinsam genutzten Bibliothekskonfiguration.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Gemeinsam genutzte Bibliothek</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="properties" type="com.ibm.ws.security.authentication.tai.interceptor.properties"> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="enabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Aktiviert oder inaktiviert den Interceptor.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Interceptor aktivieren</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="className" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Der vollständig qualifizierte Paketname der Interceptorklasse.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Klassenname</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="invokeBeforeSSO" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Ruft einen Interceptor vor Single Sign-on (SSO) auf.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Interceptor vor SSO aufrufen</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="invokeAfterSSO" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Ruft einen Interceptor nach Single Sign-on (SSO) auf.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Interceptor nach SSO aufrufen</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="libraryRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Eine Referenz auf die ID der gemeinsam genutzten Bibliothekskonfiguration.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Gemeinsam genutzte Bibliothek</ext:label> 
                    <ext:reference>library</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.authentication.tai"> 
        <xsd:annotation> 
            <xsd:documentation>Steuert die Operationen des TAI (Trust-Association-Interceptor).</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Trust-AssociationInterceptor</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="interceptors" minOccurs="0" maxOccurs="unbounded" type="com.ibm.ws.security.authentication.tai.interceptor"> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="invokeForUnprotectedURI" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Steuert, ob der TAI für einen ungeschützten URI aufgerufen wird.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>TAI für ungeschützten URI aufrufen</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="failOverToAppAuthType" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Ermöglicht einem Interceptor, auf den Authentifizierungsmechanismus der Anwendung zurückzugreifen.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Zurücksetzen auf Anwendungsauthentifizierung zulassen</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.authentication.tai-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.authentication.tai"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>Eine eindeutige Konfigurations-ID.</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.authentication.tai.interceptor.properties"> 
        <xsd:annotation> 
            <xsd:documentation>Sammlung von Eigenschaften für den Interceptor.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Interceptoreigenschaften</ext:label> 
                <ext:extraProperties/> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>Sammlung von Eigenschaften für den Interceptor.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Interceptoreigenschaften</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.app.manager.monitor"> 
        <xsd:annotation> 
            <xsd:documentation>Definiert, wie der Server auf Hinzufüge-, Aktualisierungs- und Löschoperationen in der Anwendung reagiert.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Anwendungsüberwachung</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="pollingRate" use="optional" default="500ms" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>Rate, mit der der Server nach Hinzufügungen, Aktualisierungen und Löschungen in der Anwendung sucht. Geben Sie eine positive ganze Zahl, gefolgt von einer Zeiteinheit, an. Die gültigen Zeiteinheiten sind Stunden (h), Minuten (m), Sekunden (s) und Millisekunden (ms). Geben Sie 500 Millisekunden beispielsweise als 500ms an. Sie können mehrere Werte in einen einzigen Eintrag einschließen. 1s500ms entspricht beispielsweise 1,5 Sekunden.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Abfragerate für Anwendungsaktualisierungen</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="dropins" use="optional" default="dropins" type="dirLocation"> 
            <xsd:annotation> 
                <xsd:documentation>Die Position einer Anwendung in Form eines absoluten Pfads oder eines relativen Pfads zum Serververzeichnis.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Drop-in-Verzeichnis der Anwendung</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="dropinsEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Überwacht das Drop-in-Verzeichnis auf Hinzufügungen, Aktualisierungen und Löschungen in der Anwendung.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Drop-in-Verzeichnis der Anwendung überwachen</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="updateTrigger" use="optional" default="polled"> 
            <xsd:annotation> 
                <xsd:documentation>Methode oder Auslöser für Anwendungsaktualisierungen.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Auslöser für Anwendungsaktualisierungen</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="polled"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Server sucht im Abfrageintervall nach Anwendungsänderungen und aktualisiert alle Anwendungen, in denen Änderungen erkannt wurden</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="mbean"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Server aktualisiert Anwendungen nur, wenn er dazu von einer MBean aufgefordert wird, die von einem externen Programm wie einer integrierten Entwicklungsumgebung oder einer Managementanwendung aufgerufen wird.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="disabled"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Inaktiviert die gesamte Aktualisierungsüberwachung. Es werden keine Anwendungsänderungen angewendet, solange der Server aktiv ist.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.app.manager"> 
        <xsd:annotation> 
            <xsd:documentation>Definiert die Eigenschaften einer Anwendung.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Anwendung</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader"> 
            </xsd:element> 
            <xsd:element name="application-bnd" type="com.ibm.ws.javaee.dd.appbnd"> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="location" use="required" type="location"> 
            <xsd:annotation> 
                <xsd:documentation>Die Position einer Anwendung in Form eines absoluten Pfads oder eines relativen Pfads zum Verzeichnis apps auf Serverebene.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Position</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Der Name einer Anwendung.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Name</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="type" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Der Typ des Anwendungsarchivs.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Anwendungstyp</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="context-root" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Das Kontextstammverzeichnis einer Anwendung.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Stammverzeichnis des Anwendungskontexts</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="autoStart" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Gibt an, ob der Server die Anwendung automatisch beim Serverstart starten soll.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Automatisch starten</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="suppressUncoveredHttpMethodWarning" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Option zum Unterdrücken von Warnungen zu erkannten zu HTTP-Methoden während der Anwendungsimplementierung.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Warnungen zu erkannten HTTP-Methoden unterdrücken</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.app.manager-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.app.manager"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>Eine eindeutige Konfigurations-ID.</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.classloading.global"> 
        <xsd:annotation> 
            <xsd:documentation>Globales Laden von Klassen</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Laden von Klassen</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="useJarUrls" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Angabe, ob URLs vom Typ 'jar:' zum Referenzieren von Dateien in Archiven verwendet werden sollen oder URLs vom Typ 'wsjar:'.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>URL vom Typ 'jar:' verwenden</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.basic.config.group.member"> 
        <xsd:annotation> 
            <xsd:documentation>Ein Mitglied einer Gruppe in der Basisbenutzerregistry.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Gruppenmitglied</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="name" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Der Name eines Benutzers in einer Gruppe in der Basisbenutzerregistry.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Benutzername</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.basic.config"> 
        <xsd:annotation> 
            <xsd:documentation>Eine einfache XML-basierte Benutzerregistry.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Basisbenutzerregistry</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="user" type="com.ibm.ws.security.registry.basic.config.user"> 
            </xsd:element> 
            <xsd:element name="group" type="com.ibm.ws.security.registry.basic.config.group"> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="realm" use="optional" default="BasicRegistry" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Der Realmname stellt die Benutzerregistry dar.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Realmname</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="ignoreCaseForAuthentication" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Ermöglicht die Authentifizierung des Benutzernamens ohne Beachtung der Groß-/Kleinschreibung.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Authentifizierung ohne Beachtung der Groß-/Kleinschreibung</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.basic.config-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.registry.basic.config"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>Eine eindeutige Konfigurations-ID.</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.basic.config.group"> 
        <xsd:annotation> 
            <xsd:documentation>Eine Gruppe in der Basisbenutzerregistry.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Gruppe</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="member" minOccurs="0" maxOccurs="unbounded" type="com.ibm.ws.security.registry.basic.config.group.member"> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="name" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Der Name einer Gruppe in einer Basisbenutzerregistry.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Gruppenname</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.basic.config.user"> 
        <xsd:annotation> 
            <xsd:documentation>Ein Benutzer in der Basisbenutzerregistry.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Benutzer</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="name" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Der Name eines Benutzers in einer Basisbenutzerregistry.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Benutzername</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="password" use="required" type="passwordHash"> 
            <xsd:annotation> 
                <xsd:documentation>Das Kennwort eines Benutzers in einer Basisbenutzerregistry. Der Wert kann in Klartext oder in verschlüsselter Form gespeichert werden. Es wird empfohlen, das Kennwort zu verschlüsseln. Verwenden Sie dazu das Tool securityUtility mit der Option encode.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Kennwort</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.logging.binaryTrace"> 
        <xsd:annotation> 
            <xsd:documentation>Optionen für den binären Trace. Der binäre Trace kann mit dem Befehl logViewer angezeigt werden.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Binärer Trace</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="purgeMaxSize" use="optional" default="50"> 
            <xsd:annotation> 
                <xsd:documentation>Legt die maximale Größe des Repositorys für binäre Traces in MB fest. Wenn der für purgeMaxSize angegebene Wert größer als 0 ist, ist die auf der Repository-Größe basierende Bereinigung aktiviert, andernfalls ist sie inaktiviert.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Maximale Größe des Repositorys</ext:label> 
                    <ext:variable>com.ibm.hpel.trace.purgeMaxSize</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="purgeMinTime" use="optional" default="0" type="hourDuration"> 
            <xsd:annotation> 
                <xsd:documentation>Gibt die Dauer (in Stunden) an, nach der ein Server einen Tracedatensatz entfernen kann. Wenn der für purgeMinTime angegebene Wert größer als 0 ist, ist die auf dem Tracedatensatzalter basierende Bereinigung aktiviert, andernfalls ist sie inaktiviert. Geben Sie eine positive ganze Zahl, gefolgt von der Zeiteinheit, an. Die gültige Zeiteinheit sind Stunden (h). Geben Sie 12 Stunden beispielsweise als 12h an.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Mindestaufbewahrungszeit</ext:label> 
                    <ext:variable>com.ibm.hpel.trace.purgeMinTime</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="fileSwitchTime" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>Veranlasst den Server, die aktive Tracedatei zur angegebenen Tageszeit zu schließen und ene neue zu öffnen. Wenn Sie einen Wert für fileSwitchTime angeben, ist der Dateiwechsel aktiviert, andernfalls ist er inaktiviert.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Tageszeit, zu der die Datei gewechselt wird</ext:label> 
                    <ext:variable>com.ibm.hpel.trace.fileSwitchTime</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="23"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="bufferingEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Gibt an, ob für eine bessere Leistung eine kurze Verzögerung beim Speichern von Datensätzen auf der Platte zugelassen wird. Wenn Sie bufferingEnabled auf true setzen, werden Datensätze kurzzeitig im Hauptspeicher gehalten, bevor sie auf die Platte geschrieben werden.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Ausgabepufferung aktivieren</ext:label> 
                    <ext:variable>com.ibm.hpel.trace.bufferingEnabled</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="outOfSpaceAction" use="optional" default="StopLogging"> 
            <xsd:annotation> 
                <xsd:documentation>Gibt die Aktion an, die ausgeführt werden soll, wenn im Dateisystem, in dem die Datensätze gespeichert werden, kein Speicherplatz mehr verfügbar ist. Wenn Sie outOfSpaceAction auf "StopLogging" setzen, stoppt der Server die Traceerstellung, wenn Datensätze nicht auf die Platte geschrieben werden können. Wenn Sie dieses Attribut auf "PurgeOld" setzen, versucht der Server, die ältesten Datensätze aus dem Repository für binäre Traces zu löschen, um Platz für neue Datensätze zu schaffen. Wenn Sie dieses Attribut auf "StopServer" setzen, stoppt der binäre Trace den Server, wenn die Datensätze nicht geschrieben werden können.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Aktion, wenn Daten nicht gespeichert werden können</ext:label> 
                    <ext:variable>com.ibm.hpel.trace.outOfSpaceAction</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="StopServer"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Server stoppen</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="PurgeOld"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Alte Datensätze löschen</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="StopLogging"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Protokollierung stoppen</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.logging.binaryLog"> 
        <xsd:annotation> 
            <xsd:documentation>Optionen für die Binärprotokollierung. Das Binärprotokoll kann mit dem Befehl logViewer angezeigt werden.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Binärprotokoll</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="purgeMaxSize" use="optional" default="50"> 
            <xsd:annotation> 
                <xsd:documentation>Legt die maximale Größe des Repositorys für Binärprotokolle in MB fest. Wenn der für purgeMaxSize angegebene Wert größer als 0 ist, ist die auf der Repository-Größe basierende Bereinigung aktiviert, andernfalls ist sie inaktiviert.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Maximale Größe des Repositorys</ext:label> 
                    <ext:variable>com.ibm.hpel.log.purgeMaxSize</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="purgeMinTime" use="optional" default="0" type="hourDuration"> 
            <xsd:annotation> 
                <xsd:documentation>Gibt die Dauer (in Stunden) an, nach der ein Server einen Protokolldatensatz entfernen kann. Wenn der für purgeMinTime angegebene Wert größer als 0 ist, ist die auf dem Protokolldatensatzalter basierende Bereinigung aktiviert, andernfalls ist sie inaktiviert. Geben Sie eine positive ganze Zahl, gefolgt von der Zeiteinheit, an. Die gültige Zeiteinheit sind Stunden (h). Geben Sie 12 Stunden beispielsweise als 12h an.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Mindestaufbewahrungszeit</ext:label> 
                    <ext:variable>com.ibm.hpel.log.purgeMinTime</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="fileSwitchTime" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>Veranlasst den Server, die aktive Protokolldatei zur angegebenen Tageszeit zu schließen und ene neue zu öffnen. Wenn Sie einen Wert für fileSwitchTime angeben, ist der Dateiwechsel aktiviert, andernfalls ist er inaktiviert.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Tageszeit, zu der die Datei gewechselt wird</ext:label> 
                    <ext:variable>com.ibm.hpel.log.fileSwitchTime</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="23"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="bufferingEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Gibt an, ob für eine bessere Leistung eine kurze Verzögerung beim Speichern von Datensätzen auf der Platte zugelassen wird. Wenn Sie bufferingEnabled auf true setzen, werden Datensätze kurzzeitig im Hauptspeicher gehalten, bevor sie auf die Platte geschrieben werden.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Ausgabepufferung aktivieren</ext:label> 
                    <ext:variable>com.ibm.hpel.log.bufferingEnabled</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="outOfSpaceAction" use="optional" default="StopLogging"> 
            <xsd:annotation> 
                <xsd:documentation>Gibt die Aktion an, die ausgeführt werden soll, wenn im Dateisystem, in dem die Datensätze gespeichert werden, kein Speicherplatz mehr verfügbar ist. Wenn Sie outOfSpaceAction auf "StopLogging" setzen, stoppt der Server die Protokollierung, wenn Datensätze nicht auf die Platte geschrieben werden können. Wenn Sie dieses Attribut auf "PurgeOld" setzen, versucht der Server, die ältesten Datensätze aus dem Repository für Binärprotokolle zu löschen, um Platz für neue Datensätze zu schaffen. Wenn Sie dieses Attribut auf "StopServer" setzen, stoppt das Binärprotokoll den Server, wenn die Datensätze nicht geschrieben werden können.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Aktion, wenn Daten nicht gespeichert werden können</ext:label> 
                    <ext:variable>com.ibm.hpel.log.outOfSpaceAction</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="StopServer"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Server stoppen</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="PurgeOld"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Alte Datensätze löschen</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="StopLogging"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Protokollierung stoppen</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.authentication"> 
        <xsd:annotation> 
            <xsd:documentation>Steuert die Konfiguration des integrierten Authentifizierungsservice.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Authentifizierung</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
