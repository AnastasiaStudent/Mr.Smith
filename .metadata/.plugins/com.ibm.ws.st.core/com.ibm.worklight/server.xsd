                <xsd:documentation>Ein LDAP-Filter für die Zugehörigkeiten von Benutzern zu Gruppen.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Zuordnung von Gruppen-Member-IDs</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
